@model List<TP1_ArmandoClaudio.Provincia>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Nombre</th>
        </tr>
    </thead>
    @{
        // ProvinciasArgentinas ListProv = Model;
        foreach (Provincia prov in Model)
        {
        <tr>
            <td>@prov.Id</td>
            <td>@prov.Nombre</td>
        </tr>
        }
    }

</table>